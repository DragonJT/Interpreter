
void if(bool condition, Delegate body){
    goto_if (!condition) end;
    body();
    end:
}

void loop(DelegateParameter p, int length, Delegate body){
    var i=0;
    start:
    body(i);
    i=i+1;
    goto_if (i<length) start;
}

